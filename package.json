{
  "name": "aegis-discord-bot",
  "version": "1.0.0",
  "description": "Professional Discord bot for community and enterprise use",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch -r tsconfig-paths/register src/index.ts",
    "build": "tsc",
    "start": "node start.js",
    "bot:dev": "tsx watch -r tsconfig-paths/register src/bot/index.ts",
    "bot:start": "node -r tsconfig-paths/register dist/bot/index.js",
    "api:dev": "tsx watch -r tsconfig-paths/register src/api/index.ts",
    "api:start": "node -r tsconfig-paths/register dist/api/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "db:migrate": "tsx -r tsconfig-paths/register node_modules/knex/bin/cli.js migrate:latest",
    "db:rollback": "knex migrate:rollback",
    "db:seed": "knex seed:run",
    "db:reset": "npm run db:rollback && npm run db:migrate && npm run db:seed",
    "deploy:commands": "tsx src/scripts/deploy-commands.ts"
  },
  "keywords": [
    "discord",
    "bot",
    "moderation",
    "typescript",
    "enterprise"
  ],
  "author": "Your Organization",
  "license": "MIT",
  "dependencies": {
    "@discordjs/rest": "^2.2.0",
    "bcrypt": "^5.1.1",
    "bullmq": "^4.15.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "node-cron": "^3.0.3",
    "pg": "^8.11.3",
    "prom-client": "^15.1.0",
    "redis": "^4.6.11",
    "tsconfig-paths": "^4.2.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/crypto-js": "^4.2.1",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.4",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-security": "^1.7.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
