# Copy this file to .env and fill in your values
# Environment Configuration
NODE_ENV=development
LOG_LEVEL=info
PORT=3000

# Discord Configuration
DISCORD_TOKEN=your_bot_token_here
DISCORD_CLIENT_ID=your_client_id_here
DISCORD_CLIENT_SECRET=your_client_secret_here
DISCORD_GUILD_ID=your_test_guild_id_here

# Optional: Development guild for faster command registration
DEV_GUILD_ID=your_dev_guild_id_here

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/aegis_bot
DATABASE_POOL_MIN=2
DATABASE_POOL_MAX=10

# Redis Configuration
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=your_redis_password_here

# Optional: Bot Owner ID for owner-only commands
BOT_OWNER_ID=your_user_id_here